:@has_childrenT:@template"s1@import "compass/css3";
@import "compass/reset";

/*----------------------
Partials
----------------------*/
@import "partials/animation";
@import "partials/animation/animate";


/* =====================
variables
======================*/
$bg:			#131214;
$dropHeight: 	45px;
$logo:          'data:image/png;base64,';
$picWidth:      250px;
$picMargin:     $picWidth / 6;
$wrapWidth:     ($picWidth * 3) + ($picMargin * 2);

/* =====================
app styles
======================*/
body {
	background-color:$bg;
	font-family: 'Open Sans', sans-serif;
	letter-spacing:-.01em;
	background-image:url(img/bg.png), url(img/bgPat.png);
	//background:url(img/bgPat.png);
	background-position:center -10px, 0 0;
	background-repeat:no-repeat, repeat;
}
.wrap {
	width:$wrapWidth;
	margin:auto;
}

.head {
	padding:$picMargin 0;
	overflow:hidden;
	margin-bottom:$picMargin / 2;
	.logo {
		background:url(img/logob.png) no-repeat;
		width:100px;
		height:30px;
		margin:auto;
		text-indent:-9999px;
		float:left;
		@include animation(fadeInDown);
		-webkit-animation-duration:1s;
		-webkit-animation-fill-mode:both;
		background-size:100%;
	}
	.loggedIn {
		float:right;
		width:40%;
		@include animation(fadeInRight);
		-webkit-animation-duration:1s;
		-webkit-animation-fill-mode:both;
		.avatar {
			height:30px;
			border-radius:3px;
			float:right;
			@include box-shadow(inset 0 1px 0 #fff);
			margin-left:5px;
		}
		.name {
			text-align:right;
			color:#fff;
			font-weight:700;
			font-size:80%;
		}
		.stats {
			text-align:right;
			color:#ccc;
			font-size:70%;
		}
	}
}

.pics {
	ul {
		@include animation(fadeInUp 1s ease-in);
		li {
			-webkit-transform: rotate3d(0,0,0,0);
			width:$picWidth;
			height:$picWidth + $picMargin + ($picMargin / 3);
			float:left;
			padding-right:$picMargin;
			position:relative;
			&.animate {
				-webkit-animation-duration:1s;
				-webkit-animation-fill-mode:both;
			}
			&.fadeInUp{   
				@include animation-name(fadeInUp)
			}
			&.bounceIn {
				@include animation-name(bounceIn);
			}
			&:nth-child(3n+3) {
				padding-right:0;
			}
			.highlight {
				position:relative;

				img {
					width:$picWidth;
					height:$picWidth;
					border-radius:1px;
					position:relative;
					top:0;
					left:0;
					z-index:5;
					@include transition(all .3s ease-in-out);
					+ a {
						box-shadow:inset 0px 1px 0px rgba(255,255,255,.5);
					    position: absolute; 
					    width: $picWidth;
					    height: $picWidth;
					    top: 0;
					    left: 0;
					    z-index:6;
					    @include transition(all .3s ease-in-out);
					    &.reveal {
							top:$dropHeight;
						}	
					}
					&.reveal {
						top:$dropHeight;
					}
				}
			}
			
		}
	}
	.user {
		z-index:1;
		position:absolute;
		top:0;
		left:0;
		height:$dropHeight - 20px;
		padding:10px 0;
		width: $picWidth;
		img {
			height:25px;
			border-radius:3px;
			float:left;
			@include box-shadow(inset 0 1px 0 #fff);
		}
		span.name {
			color:#fff;
			font-weight:bold;
			float:left;
			font-size:80%;
			padding:4px 0 0 5px;

		}
		.like {
			width:25px;
			height:24px;
			background:url(img/heart.png) center top no-repeat;
			float:right;
			display:inline-block;
			cursor:pointer;
			&:hover {
				background:url(img/heart.png) center bottom no-repeat;
			}	
		}
		.heartAni {
			width:25px;
			height:24px;
			background-image:url(img/heartLrg.png);
			background-repeat:no-repeat;
			background-size:100%;
			position:absolute;
			// for when Chrome supports animates on after classes 
			//
			//@include animation(fadeOut);
			//-webkit-animation-duration:2s;
			//-webkit-animation-fill-mode:both;
		}
	}
}

.footer {
	width:100%;
	overflow:hidden;
	@include animation(fadeInUp 1.5s ease-in);
	.more {
		border: 1px solid lighten($bg, 3%);
		font-weight:700;
		font-size: 12px;
		padding: 7px 10px;
		white-space: nowrap;
		vertical-align: middle;
		color: lighten($bg, 10%);
		background: transparent;
		cursor: pointer;
		border-radius:3px;
		border: 1px solid lighten($bg, 5%);
		@include box-shadow(0 1px 0 lighten($bg, 10%));
		//@include transition(all .15s ease-in-out);
		display: block;
		position:absolute;
		left:50%;
		margin-left:-30px;
		text-decoration:none;
		&:hover {
			border-color: #999;
			background: -webkit-linear-gradient(top, white, #E0E0E0);
			box-shadow: 0 1px 2px rgba(0, 0, 0, 0.5), inset 0 0 3px white;
			text-decoration: none;
			color: lighten($bg, 20%);
			text-shadow:0 1px 1px white;
		}
	}
	.settings {
		text-indent:-9999px;
		background:url(img/settings.png) no-repeat;
		width:28px;
		height:28px;
		float:right;
		opacity:.2;
		background-size:100%;
		@include transition(all .25s 4ms both ease-in-out);
		&:hover {
			opacity:.5;
		}
	}
}:
