:@template"R2@import "compass/css3";
@import "compass/reset";

/* =====================
variables
======================*/
$bg:			#131214;
$dropHeight: 	45px;
$logo:          'data:image/png;base64,';
$picWidth:      280px;
$picMargin:     $picWidth / 6;
$wrapWidth:     ($picWidth * 3) + ($picMargin * 2);
$blue:			#68a4f5;


/*----------------------
Partials
----------------------*/
@import "partials/animation";
@import "partials/animation/animate";
@import "partials/flipper";

/* =====================
app styles
======================*/
body {
	background-color:$bg;
	font-family: 'Open Sans', sans-serif;
	letter-spacing:-.01em;
	background-image:url(img/bg.png), url(img/bgPat.png);
	//background:url(img/bgPat.png);
	background-position:center -10px, 0 0;
	background-repeat:no-repeat, repeat;
}

a {
	color:$blue;
	text-decoration:none;
	outline:none;
	&:hover {
		color:#ce4d73;
	}
}

.wrap {
	width:$wrapWidth;
	margin:auto;
}

.head {
	padding:$picMargin 0;
	overflow:hidden;
	margin-bottom:$picMargin / 2;
	.logo {
		background:url(img/logob.png) no-repeat;
		width:100px;
		height:30px;
		margin:auto;
		text-indent:-9999px;
		float:left;
		@include animation(fadeInDown);
		-webkit-animation-duration:1s;
		-webkit-animation-fill-mode:both;
		background-size:100%;
	}
	.loggedIn {
		float:right;
		width:40%;
		@include animation(fadeInRight);
		-webkit-animation-duration:1s;
		-webkit-animation-fill-mode:both;
		.avatar {
			height:30px;
			border-radius:3px;
			float:right;
			@include box-shadow(inset 0 1px 0 #fff);
			margin-left:5px;
		}
		.name {
			text-align:right;
			color:#fff;
			font-weight:700;
			font-size:80%;
			white-space: nowrap;
			overflow: hidden;
			text-overflow: ellipsis;
			width: 120px;
		}
		.stats {
			text-align:right;
			color:#ccc;
			font-size:70%;
		}
	}
}

.pics {
	//overflow:hidden;
	ul {
		@include animation(fadeInUp 1s ease-in);
		li {
			-webkit-transform: rotate3d(0,0,0,0);
			width:$picWidth;
			//height:$picWidth + $picMargin + ($picMargin / 3);
			margin-bottom:$picMargin;
			float:left;
			padding-right:$picMargin - 1;
			position:relative;
			&.animate {
				-webkit-animation-duration:1s;
				-webkit-animation-fill-mode:both;
			}
			&.fadeInUp{   
				@include animation-name(fadeInUp)
			}
			&.bounceIn {
				@include animation-name(bounceIn);
			}
			&:nth-child(3n+3) {
				padding-right:0;
			}
			.picture {
				position:relative;
			}
			
		}
	}
}

.footer {
	width:100%;
	overflow:hidden;
	@include animation(fadeInUp 1.5s ease-in);
	.more {
		border: 1px solid lighten($bg, 3%);
		font-weight:700;
		font-size: 12px;
		padding: 7px 10px;
		white-space: nowrap;
		vertical-align: middle;
		color: lighten($bg, 10%);
		background: transparent;
		cursor: pointer;
		border-radius:3px;
		border: 1px solid lighten($bg, 5%);
		@include box-shadow(0 1px 0 lighten($bg, 10%));
		//@include transition(all .15s ease-in-out);
		display: block;
		position:absolute;
		left:50%;
		margin-left:-30px;
		text-decoration:none;
		&:hover {
			border-color: #999;
			background: -webkit-linear-gradient(top, white, #E0E0E0);
			box-shadow: 0 1px 2px rgba(0, 0, 0, 0.5), inset 0 0 3px white;
			text-decoration: none;
			color: lighten($bg, 20%);
			text-shadow:0 1px 1px white;
		}
	}
	.settings {
		text-indent:-9999px;
		background:url(img/settings.png) no-repeat;
		width:28px;
		height:28px;
	  	display:block;
	  	margin:auto;
		opacity:.4;
		background-size:100%;
		@include transition(all .25s 4ms both ease-in-out);
		&:hover {
			opacity:.8;
		}
	}
}

/* =====================
Settings Styles
======================*/

.overlay {
	position:absolute;
	top:0;
	left:0;
	right:0;
	bottom:0;
	display:none;
}

.content {
	position:relative;
	width:$wrapWidth;
	margin:auto;
	margin-top:5%;
	z-index:100;
	&.in {
		@include animation(fadeInDown 1s 500ms both cubic-bezier(0, 0, .45, 1));
	}
	&.out {
		@include animation(fadeOutUp .5s 0ms both cubic-bezier(0, 0, .45, 1));
	}
	input {
		border:0;
		background:transparent;
		font-size:2em;
		font-weight:200;
		letter-spacing:-.01em;
		color:$blue;
		font-family: 'Open Sans', sans-serif;
		padding-top:.5em;
		border-bottom:1px dashed #333;
		width:60%;
		&:focus {
			outline:none;
		}
		&.error {
			color:#ce4d73;
		}
		//&.playerList {
		//	color:#81ce4d;
		//}	
	}
	h2 {
		font-weight:200;
		font-size:3em;
		padding-top:1em;
		color:#fff;
	}
	p {
		color:#ccc;
		padding-top:1em;
		width:65%;
		line-height:1.4em;
		&.subInfo {
			font-size:.8em;
			color:#999;
		}
	}
	.close {
		text-indent:-9999px;
		background:url(img/close.png) no-repeat;
		width:28px;
		height:28px;
	  	display:block;
	  	margin:auto;
		opacity:.4;
		background-size:100%;
		margin-top:70px;
		cursor:pointer;
		@include transition(all .25s 4ms both ease-in-out);
		&:hover {
			opacity:.6;
		}
	}
}
.overlayBg {
	position:absolute;
	top:0;
	left:0;
	right:0;
	bottom:0;
	background:#000;
	opacity:.95;
	z-index:50;
}

.clear {
	clear:both;
}
